{
  "extends": "tslint:recommended",
  "rules": {
    "forin": false,
    "interface-name": {
      "options": "never-prefix"
    },
    "member-ordering": false,
    "no-console": false,
    "no-namespace": {
      "options": "allow-declarations"
    },
    "trailing-comma": {
      "options": {
        "multiline": "never",
        "singleline": "never"
      }
    },
    "triple-equals": false,
    "variable-name": {
      "options": [
        "ban-keywords",
        "check-format",
        "allow-pascal-case",
        "allow-leading-underscore"
      ]
    }
  }
}
